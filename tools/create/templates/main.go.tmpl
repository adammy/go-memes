package main

import (
	"github.com/adammy/memepen-services/pkg/{{.name}}"
	"github.com/adammy/memepen-services/pkg/httpapi"
)

func main() {
	config, err := httpapi.LoadConfig[{{.name}}.Config](httpapi.ConfigPath, "{{.name}}")
	if err != nil {
		panic(err)
	}

	server := {{.name}}.NewServer(config)
	if err := server.Start(); err != nil {
		panic(err)
	}
}
